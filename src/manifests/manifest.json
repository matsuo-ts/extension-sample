{
  "manifest_version": 2,

  "name": "Bar検索",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "icons": {
    "16": "beer.png"
  },
  "browser_action": {
    "default_name": "test extension",
    "default_icon": "beer.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}